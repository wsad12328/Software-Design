package code;

public class Main {
	public class Main {
		 public static void main(String[] args) {
			 Dog myDog= newDog(); 
			 Cat myCat= newCat(); 
			 Sheep mySheep= new Sheep(); 
			 myDog.sound(); // Outputs: Dog barks
			 myCat.sound(); // Outputs: Cat meows
			 mySheep.sound(); // Outputs: Sheep bleats
		 }
	}
}
